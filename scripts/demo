# Create demo directory
mkdir -p demo

# Create a 50MB test file (this works in Git Bash)
dd if=/dev/zero of=demo/file-50mb.bin bs=1048576 count=50 2>/dev/null

# Verify the file was created
ls -lh demo/file-50mb.bin

# Run your command again
bash demo/run.sh -m archipel.cli seed --file demo/file-50mb.bin --port 38834
